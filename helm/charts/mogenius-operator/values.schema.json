                        "capabilities": {
                            "type": "object",
                            "properties": {
                                "add": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        },
                        "privileged": {
                            "type": "boolean"
                        },
                        "readOnlyRootFilesystem": {
                            "type": "boolean"
                        }
                    }
                },
                "enabled": {
                    "type": "boolean"
                },
                "nodeSelector": {
                    "type": "object"
                },
                "podLabels": {
                    "type": "object"
                },
                "podSecurityContext": {
                    "type": "object"
                },
                "resources": {
                    "type": "object"
                },
                "tolerations": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "effect": {
                                "type": "string"
                            },
                            "key": {
                                "type": "string"
                            },
                            "operator": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "nodeSelector": {
            "type": "object"
        },
        "podLabels": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "probes": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "livenessProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
