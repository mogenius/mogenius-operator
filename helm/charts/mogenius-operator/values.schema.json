                    }
                },
                "readinessProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "path": {
                            "type": "string"
                        }
                    }
                },
                "startupProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "path": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "resources": {
            "type": "object"
        },
        "revisionHistoryLimit": {
            "type": "integer"
